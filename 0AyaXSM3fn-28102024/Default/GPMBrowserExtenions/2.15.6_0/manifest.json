{
   "action": {
      "default_popup": "popup.html",
      "default_title": "Bitget"
   },
   "background": {
      "service_worker": "static/js/background.js"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "chromeos": "Alt+Shift+K",
            "linux": "Alt+Shift+K",
            "mac": "Alt+Shift+K",
            "windows": "Alt+Shift+K"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "static/js/contentscript.js", "static/js/bundle.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';  frame-ancestors 'none';"
   },
   "default_locale": "en",
   "description": "__MSG_appDescription__",
   "externally_connectable": {
      "ids": [ "*" ],
      "matches": [ "https://bitkeep.com/*", "https://web3.bitget.com/" ]
   },
   "host_permissions": [ "http://localhost/*", "*://*/*" ],
   "icons": {
      "128": "static/images/favicon.png",
      "16": "static/images/favicon.png",
      "19": "static/images/favicon.png",
      "32": "static/images/favicon.png",
      "48": "static/images/favicon.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "__MSG_appName__",
   "permissions": [ "storage", "activeTab", "notifications", "unlimitedStorage", "scripting", "tabs", "alarms" ],
   "short_name": "__MSG_appName__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.15.6"
}
